<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.cab302project.FocusSessPageController">
   <children>
      <ProgressIndicator fx:id="clock" layoutX="286.0" layoutY="210.0" progress="1.0" scaleX="6.0" scaleY="6.0" scaleZ="6.0" />
      <Text fx:id="timeText" fill="#404040" layoutX="209.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="00:00:00" textAlignment="CENTER">
         <font>
            <Font size="50.0" />
         </font>
      </Text>
      <Button fx:id="controlSess" alignment="BOTTOM_CENTER" layoutX="177.0" layoutY="339.0" mnemonicParsing="false" onAction="#handleControlSess" prefHeight="37.0" prefWidth="98.0" text="Pause">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="endSess" alignment="BOTTOM_CENTER" layoutX="332.0" layoutY="339.0" mnemonicParsing="false" onAction="#handleEndSess" prefHeight="37.0" prefWidth="98.0" text="End Session">
         <font>
            <Font size="14.0" />
         </font>
      </Button>

   </children>
</AnchorPane>
